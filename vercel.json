{
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": { "distDir": "build" }
		}
	],
	"routes": [
		{
			"src": "/_app/.+",
			"headers": { "cache-control": "public, immutable, max-age=31536000" }
		},
		{ "src": "/(.*)", "dest": ".vercel/serverless/sveltekit-deno" }
	]
}
